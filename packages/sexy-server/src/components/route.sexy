<dynamic :is="tag" @click="navigate">
	<slot></slot>
</dynamic>

<script>
import { router } from '../internal/router';

let tag = p('a');
let to = p(null);
let history = p(true);

function navigate() {
	if(history()) {
		router.navigate(to());
	} else {
		window.location.href = to();
	}
}
</script>
